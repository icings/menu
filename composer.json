{
    "name": "icings/menu",
    "type": "cakephp-plugin",
    "description": "A KnpMenu seasoned menu plugin for CakePHP.",
    "keywords": ["cakephp", "menu", "knp-menu"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Nowak",
            "role": "Author",
            "homepage": "https://nowak-media.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.0",
        "cakephp/cakephp": "^4.0",
        "knplabs/knp-menu": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.5.0",
        "cakephp/cakephp-codesniffer": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Icings\\Menu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icings\\Menu\\Test\\": "tests",
            "Icings\\Menu\\Test\\TestApp\\": "tests/TestApp/src",
            "Knp\\Menu\\Tests\\": "vendor/knplabs/knp-menu/tests/Knp/Menu/Tests"
        }
    },
    "scripts": {
        "check": [
            "@cs-check -- -p",
            "@test"
        ],
        "cs-check": "phpcs --parallel=16 ./src ./tests",
        "cs-fix": "phpcbf --parallel=16 -p ./src ./tests",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-clover=coverage.xml",
        "stan-setup": "cp composer.json composer.backup && composer require --dev \"phpstan/phpstan:^0.12.0\" \"psalm/phar:^4.0\" && mv composer.backup composer.json",
        "stan": [
            "@phpstan-src",
            "@phpstan-tests",
            "@psalm"
        ],
        "phpstan": [
            "@phpstan-src",
            "@phpstan-tests"
        ],
        "phpstan-src": "php ./vendor/phpstan/phpstan/phpstan.phar analyse -l 7 -c ./phpstan.src.neon ./src",
        "phpstan-tests": "php ./vendor/phpstan/phpstan/phpstan.phar analyse -l 7 -c ./phpstan.tests.neon ./tests",
        "psalm": "php ./vendor/psalm/phar/psalm.phar"
    }
}
